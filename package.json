{
  "name": "abacus",
  "version": "0.1.0",
  "scripts": {
    "build": "npx ts-node bin/clear-layers.ts && mkdir \"bin/layers/jsdom/nodejs/node_modules\" && cd bin/layers/jsdom/nodejs && npm install jsdom@^24.0.0 --no-bin-links --omit=optional --no-package-lock --no-save",
    "crawl": "npx ts-node --prefer-ts-exts bin/crawl.ts",
    "deploy": "npm run synth && cdk deploy",
    "test": "jest",
    "synth": "npm run build && cdk synth"
  },
  "engines": {
    "node": ">=20.11.1"
  },
  "dependencies": {
    "aws-cdk-lib": "2.128.0",
    "aws-sdk": "^2.1564.0",
    "constructs": "^10.0.0",
    "jsdom": "^24.0.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.16",
    "aws-cdk": "2.128.0",
    "esbuild": "^0.20.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  }
}
